N= int(input('N: '))
M= int(input('M: '))
board=[[0]*M for _ in range(N)]
counter=1
k=1
h=0
while counter<=M*N or h<N:
    if k>0:
        for i in range(N):
            board[i][h]=counter
            counter+=1
            for j in board: print(j)
            print()
    else:
        for i in range(N-1, -1, -1):
            board[i][h]=counter
            counter+=1
            for j in board: print(j)
            print()
    k*=-1
    h+=1
for i in board:
    print(*i)
